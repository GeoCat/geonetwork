// NOTE: lines starting with "//" are pre-processed out of this file so comments are fine.
//
{
  "id": "",
  "title": "",

  "facets": {

    "type": {
      "type": "term",
      "property": "type",
      "x-elastic-property": "resourceType",
      "sortedBy": "count",
      "minOccurs": 1
    },


    "spatialRepresentationType": {
      "type": "term",
      "property": "spatialRepresentationType",
      "x-elastic-property": "cl_spatialRepresentationType.key",
      "sortedBy": "count",
      "minOccurs": 1
    },

    "format": {
      "type": "term",
      "property": "format",
      "x-elastic-property": "format",
      "sortedBy": "count",
      "minOccurs": 1,
      "bucketCount": 50
    },

    "keywords": {
      "type": "term",
      "property": "keywords",
      "x-elastic-property": "tag.langeng",
      "sortedBy": "count",
      "minOccurs": 1,
      "bucketCount": 50
    },


    "updateFrequency": {
      "type": "term",
      "property": "updateFrequency",
      "x-elastic-property": "cl_maintenanceAndUpdateFrequency.key",
      "sortedBy": "count",
      "minOccurs": 1,
      "bucketCount": 50
    },



    "orgForResource": {
      "type": "term",
      "property": "orgForResource",
      "x-elastic-property": "OrgForResourceObject.default",
      "sortedBy": "count",
      "minOccurs": 1,
      "bucketCount": 50
    },


    "availableInServices": {
      "type": "filter",
      "property": "linkProtocol",
      "filters": {
        "availableInDownloadService": "linkProtocol = 'OGC:WFS'",
        "availableInViewService": "linkProtocol = 'OGC:WMS'"
      }
    },

        "resolutionScaleDenominator": {
          "type": "histogram",
          "property": "resolutionScaleDenominator",
          "bucketType": "fixedInterval",
          "sortedBy": "value",
          "bucketCount": 25,
          "x-elastic-property": "resolutionScaleDenominator",
          "x-elastic-datatype": "number",
          "x-interval-number" : 10000,
          "x-minimum-doc-count": 1
        },

    "creationYearForResource": {
      "type": "histogram",
      "property": "creationYearForResource",
      "bucketType": "fixedInterval",
      "sortedBy": "value",
      "bucketCount": 25,
      "x-elastic-property": "creationYearForResource",
      "x-elastic-datatype": "number",
      "x-interval-number" : 5,
      "x-minimum-doc-count": 1
    },

    "creationYearForResource2": {
      "type": "histogram",
      "property": "creationYearForResource",
      "bucketType": "fixedBucketCount",
      "sortedBy": "value",
      "bucketCount": 5,
      "x-elastic-property": "creationYearForResource",
      "x-elastic-datatype": "number"
    },

    "createDate": {
      "type": "histogram",
      "property": "createDate",
      "bucketType": "fixedInterval",
      "sortedBy": "value",
      "bucketCount": 25,
      "x-elastic-property": "createDate",
      "x-elastic-datatype": "date",
      "x-interval-calendar-interval" : "month",
      "x-minimum-doc-count": 1
    },

    "createDate2": {
      "type": "histogram",
      "property": "createDate",
      "bucketType": "fixedBucketCount",
      "sortedBy": "value",
      "bucketCount": 5,
      "x-elastic-property": "createDate",
      "x-elastic-datatype": "date",

      "x-minimum-doc-count": 1
    }




  },
  "defaultBucketCount": 10

}
